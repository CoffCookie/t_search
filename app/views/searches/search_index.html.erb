<%= render 'part_common/search_text_field' %>

<div class= "base_layout">
    <% if !@search_result.blank? %>
        <% @search_result.each do |result| %>
            <%= result.category %><br>
            <%= result.title %><br>
            <a href="<%= result.url %>"><%= result.url %></a><br>
            <%= result.description %><br><br>
        <% end %>
    <% else %>
        <%= @unsearch_result %>
    <% end %>
</div>